<h1>Galpão</h1>
<h2><%= @warehouse.name %></h2>
<%= flash[:notice] %>
<dl>
<dt>Código:</dt>
<dd><%= @warehouse.code %></dd>
<dt>Descrição:</dt>
<dd><%= @warehouse.description %></dd>
<dt>Endereço:</dt>
<dd><%= @warehouse.address %>
<%= @warehouse.city %>/<%= @warehouse.state %>
CEP: <%= @warehouse.postal_code %></dd>
<dt>Área Total:</dt>
<dd><%= @warehouse.total_area %> m2</dd>
<dt>Área Útil:</dt>
<dd><%= @warehouse.useful_area %> m2</dd>
</dl>
<%= link_to 'Voltar', root_path %>
